{
  "name": "bthwani-platform",
  "version": "1.0.0",
  "description": "BThwani Platform - Mono-repository for specifications, runtime code, and governance",
  "main": "dist/main.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:dsh": "jest --config ./test/jest-e2e.json test/e2e",
    "test:integration": "jest --config ./test/jest-e2e.json test/integration",
    "test:performance": "jest --config ./test/jest-e2e.json test/performance",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:app-user": "playwright test --project=app-user",
    "test:playwright:app-partner": "playwright test --project=app-partner",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migration:pending": "mikro-orm migration:pending",
    "migration:fresh": "mikro-orm migration:fresh",
    "orm:migrate:dry": "node scripts/wave/migrations-dry-run.js",
    "seed:dry": "node scripts/wave/seeds-dry-run.js",
    "build:openapi": "node scripts/build-master-openapi.js",
    "check:openapi": "node scripts/build-master-openapi.js --check",
    "contracts": "npm run build:openapi && spectral lint oas/**/*.yaml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bthwani1/bthwani-platform.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/bthwani1/bthwani-platform/issues"
  },
  "homepage": "https://github.com/bthwani1/bthwani-platform#readme",
  "dependencies": {
    "@mikro-orm/core": "^6.6.0",
    "@mikro-orm/migrations": "^6.6.0",
    "@mikro-orm/nestjs": "^6.1.1",
    "@mikro-orm/postgresql": "^6.6.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.1.17",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.6.0",
    "@mikro-orm/sqlite": "^6.6.0",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@stoplight/spectral-cli": "^6.15.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "codecov": "^3.8.2",
    "commander": "^14.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nx": "^18.0.3",
    "playwright": "^1.40.0",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "yaml": "^2.8.1"
  }
}
